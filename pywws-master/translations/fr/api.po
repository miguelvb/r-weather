#: ../../pywws/calib.py:docstring of pywws.calib:10
msgid "Generate relative atmospheric pressure."
msgstr "Générer pression atmosphérique relative."

# 455d362f48f845879da83b9c29b83acc
#: ../../pywws/calib.py:docstring of pywws.calib:11
msgid "Remove invalid wind direction values."
msgstr "Retirer les valeurs invalides de direction du vent."

# 38a401d8f4974122851fcc95089c8da6
#: ../../pywws/calib.py:docstring of pywws.calib:13
msgid "Any user calibration you write must also do these."
msgstr ""
"Toute calibration utilisateur que vous écrivez doit également faire ceci."

# 8fce31f454594d6d80ca2cd414c0532e
